model EarlyAccess {
  id        String   @id @default(cuid())
  name      String
  email     String
  phone     String?
  message   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("early_access")
}

model FounderContact {
  id        String   @id @default(cuid())
  name      String
  email     String
  phone     String?
  message   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("founder_contact")
}
